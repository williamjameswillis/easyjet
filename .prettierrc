{
  "tabWidth": 2,
  "parser": "typescript",
  "arrowParens": "always",
  "quoteProps": "consistent",
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "overrides": [
    {
      "files": ".prettierrc",
      "options": { "parser": "json" }
    },
    {
      "files": "*.json",
      "options": { "parser": "json" }
    }
  ]
}
